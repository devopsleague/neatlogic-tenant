<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="codedriver.module.tenant.dao.mapper.MenuMobileMapper">
    <cache type="codedriver.framework.dao.cache.CodeDriverCache" flushInterval="30000" size="100"/>
    <select id="getMenuMobileList" resultType="codedriver.module.tenant.dto.MenuMobileVo">
        select name, label, icon, config as configStr
        from menu_mobile
    </select>
</mapper>

